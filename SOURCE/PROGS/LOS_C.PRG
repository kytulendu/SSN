 SELECT los.dg1, los.desc, COUNT(los.an), AVG(los.exp_6) FROM LOS GROUP BY los.dg1 ORDER BY los.dg1 INTO DBF LOS_DX.DBF
 CREATE DBF LOS_NO (dg1 C (5), desc C (84), cnt N (10, 0), avg_exp_6 N (16, 0))
 no = 1
 SELECT los_dx
 INDEX ON avg_exp_6 TAG los OF los_dx DESCENDING
 SET ORDER TO los
 GOTO TOP
 DO WHILE .T.
    IF no=16
       EXIT
    ENDIF
    SELECT los_dx
    SCATTER MEMVAR
    INSERT INTO los_no FROM MEMVAR
    no = no+1
    SKIP
 ENDDO
 SELECT * FROM LOS_NO INTO CURSOR LOS_NO
 CREATE REPORT LOS_NO.FRX FROM LOS_NO COLUMN NOOVERWRITE WIDTH 150
 REPORT FORMAT LOS_NO.FRX PREVIEW
 CLOSE DATABASES
 DELETE FILE los_dx.dbf
 DELETE FILE los_dx.cdx
 DELETE FILE los_no.dbf
*
